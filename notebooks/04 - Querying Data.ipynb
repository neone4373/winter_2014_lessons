     "cell_type": "code",
     "collapsed": false,
     "input": [
      "slg = lambda x: (x['h']-x['X2b']-x['X3b']-x['hr'] + 2*x['X2b'] + 3*x['X3b'] + 4*x['hr'])/(x['ab']+1e-6)\n",
      "p3d = lambda x: '%.3f' % x \n",
      "baseball['SLG'] = baseball.apply(slg, axis=1).apply(p3d)\n",
      "baseball['SLG']"
     ],
